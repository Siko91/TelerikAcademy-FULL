<Window x:Class="MongoDbChat.WpfClient.InputUsernameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="InputUsernameWindow" Height="134" Width="210">
    <Grid Margin="0,0,2,-4" Background="{DynamicResource {x:Static SystemColors.ControlDarkDarkBrushKey}}">
        <Button Content="LogIn" HorizontalAlignment="Left" Margin="10,49,0,0" VerticalAlignment="Top" Width="180" Height="49" Click="Button_Click_1"/>
        <TextBox HorizontalAlignment="Left" x:Name="UsernameTextBox" Height="34" Margin="10,10,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="180"/>

    </Grid>
</Window>
